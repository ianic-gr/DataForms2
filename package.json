{
  "name": "@ianicdev/dataforms2",
  "version": "1.16.1",
  "type": "module",
  "types": "./lib/index.d.ts",
  "homepage": "https://ianic-gr.github.io/DataForms2/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianic-gr/DataForms2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./dist/dataforms.js",
      "require": "./dist/dataforms.cjs"
    },
    "./styles.css": "./dist/dataforms.css",
    "./locale": {
      "types": "./lib/locale/index.d.ts",
      "import": "./lib/locale/index.js",
      "require": "./lib/locale/index.js"
    }
  },
  "module": "./dist/dataforms.js",
  "main": "./dist/dataforms.cjs",
  "browser": {
    "./styles.css": "./dist/dataforms.css"
  },
  "license": "MIT",
  "files": [
    "dist/",
    "lib/",
    "lib/index.d.ts",
    "lib/locale.d.ts",
    "lib/locale/index.d.ts"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "lib/index.d.ts"
      ],
      "locale": [
        "lib/locale.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist && vue-tsc --noEmit && vite build",
    "build:pages": "cross-env BUILD_TYPE=pages vite build && touch ./dist/.nojekyll && cp ./dist/index.html ./dist/404.html",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "deploy": "gh-pages -d dist",
    "update": "npx npm-check-updates --interactive --format group"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@mdi/font": "7.4.47",
    "@stackblitz/sdk": "^1.11.0",
    "@uivjs/vue-markdown-preview": "^1.0.5",
    "@vee-validate/i18n": "^4.15.1",
    "@vee-validate/rules": "^4.15.1",
    "core-js": "^3.47.0",
    "cross-env": "^10.1.0",
    "defu": "^6.1.4",
    "moment": "^2.30.1",
    "roboto-fontface": "*",
    "uuid": "^13.0.0",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.25",
    "vue-i18n": "^11.2.1",
    "vuetify": "^3.10.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tsconfig/node22": "^22.0.5",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "defu": "^6.1.4",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.6.0",
    "gh-pages": "^6.3.0",
    "pinia": "^3.0.4",
    "sass": "^1.94.2",
    "typescript": "^5.9.3",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-fonts": "^1.4.0",
    "unplugin-vue-components": "^30.0.0",
    "unplugin-vue-router": "^0.17.1",
    "vite": "^7.2.4",
    "vite-plugin-vue-layouts-next": "^1.2.0",
    "vite-plugin-vuetify": "^2.1.2",
    "vue-router": "^4.6.3",
    "vue-tsc": "^3.1.5"
  }
}
