    "core-js": "^3.41.0",
    "cross-env": "^7.0.3",
    "defu": "^6.1.4",
    "moment": "^2.30.1",
    "roboto-fontface": "*",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
